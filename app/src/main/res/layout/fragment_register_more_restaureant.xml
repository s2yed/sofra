<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     tools:ignore="UnusedAttribute,RtlSymmetry,ContentDescription">


    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dim_64"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/registerMoreRestaurantActivityImgNew"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dim_96"

                android:layout_marginTop="24dp"
                android:src="@drawable/new_user"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_gravity="center"
                />

            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingStart="@dimen/txt_size_32"
                android:paddingEnd="@dimen/txt_size_32"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/registerMoreRestaurantActivityEditName"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dim_40"
                    android:layout_marginTop="@dimen/dim_8"
                    android:drawableStart="@drawable/icon_password"
                    android:drawablePadding="@dimen/dim_4"
                    android:drawableTint="@color/dark"
                    android:hint="@string/name_restaurant"
                    android:inputType="text"
                    android:paddingStart="@dimen/dim_8"
                    android:textColorHint="@color/dark"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/registerMoreRestaurantActivityEditEmail"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dim_40"
                    android:layout_marginTop="@dimen/dim_8"
                    android:drawableStart="@drawable/icon_email"
                    android:drawablePadding="@dimen/dim_4"
                    android:drawableTint="@color/dark"
                    android:hint="@string/email"
                    android:inputType="textEmailAddress"
                    android:paddingStart="@dimen/dim_8"
                    android:textColorHint="@color/dark"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/registerMoreRestaurantActivityEditName" />

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/registerMoreRestaurantEditPhone"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dim_40"
                    android:layout_marginTop="@dimen/dim_8"
                    android:drawableStart="@drawable/icon_phone"
                    android:drawablePadding="@dimen/dim_4"
                    android:drawableTint="@color/dark"
                    android:hint="@string/phone"
                    android:inputType="date"
                    android:paddingStart="@dimen/dim_8"
                    android:textColorHint="@color/dark"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/registerMoreRestaurantActivityEditEmail"
                    tools:layout_editor_absoluteY="212dp" />


                <Spinner
                    android:id="@+id/registerMoreRestaurantActivitySpinnerCite"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dim_40"
                    android:layout_marginTop="@dimen/dim_8"
                    android:background="@drawable/shape_spinner_dark"
                    android:gravity="center"
                    android:spinnerMode="dropdown"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/registerMoreRestaurantEditPhone" />

                <Spinner
                    android:id="@+id/registerMoreRestaurantActivityRegionsSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dim_8"
                    android:background="@drawable/shape_spinner_dark"
                    android:gravity="center"
                    android:spinnerMode="dropdown"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"

                    app:layout_constraintTop_toBottomOf="@+id/registerMoreRestaurantActivitySpinnerCite" />


                <android.support.design.widget.TextInputEditText
                    android:id="@+id/registerMoreRestaurantActivityEditPassword"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dim_40"
                    android:layout_marginTop="@dimen/dim_8"
                    android:drawableStart="@drawable/icon_password"
                    android:drawablePadding="@dimen/dim_4"
                    android:drawableTint="@color/dark"
                    android:hint="@string/password"
                    android:inputType="textPassword"
                    android:paddingStart="@dimen/dim_8"
                    android:textColorHint="@color/dark"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"

                    app:layout_constraintTop_toBottomOf="@+id/registerMoreRestaurantActivityRegionsSpinner" />

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/registerMoreRestaurantActivityEmphasisEditPassword"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dim_40"
                    android:layout_marginTop="@dimen/dim_8"
                    android:drawableStart="@drawable/icon_password"
                    android:drawablePadding="@dimen/dim_4"
                    android:drawableTint="@color/dark"
                    android:hint="@string/confirm_password"
                    android:inputType="textPassword"
                    android:paddingStart="@dimen/dim_8"
                    android:textColorHint="@color/dark"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.25"
                    app:layout_constraintStart_toStartOf="parent"

                    app:layout_constraintTop_toBottomOf="@+id/registerMoreRestaurantActivityEditPassword" />

                <Button
                    android:id="@+id/registerMoreRestaurantActivityBtnContinues"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dim_8"
                    android:layout_marginBottom="@dimen/dim_64"
                    android:background="@drawable/shape_btn"
                    android:text="@string/continues"
                    android:textColor="@color/dark"
                    android:textSize="@dimen/txt_size_16"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/registerMoreRestaurantActivityEmphasisEditPassword" />

                <ProgressBar
                    android:id="@+id/progressBarRestaurant"
                    style="?android:attr/progressBarStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="@+id/registerMoreRestaurantActivityBtnContinues"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/registerMoreRestaurantActivityEditName"
                    tools:visibility="gone" />

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dim_8"
                    android:src="@drawable/ic_home_black_24dp"
                    android:tint="@color/dark"
                    app:layout_constraintBottom_toBottomOf="@+id/registerMoreRestaurantActivityRegionsSpinner"
                    app:layout_constraintStart_toStartOf="@+id/registerMoreRestaurantActivityRegionsSpinner"
                    app:layout_constraintTop_toTopOf="@+id/registerMoreRestaurantActivityRegionsSpinner" />

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dim_8"
                    android:src="@drawable/ic_home_black_24dp"
                    android:tint="@color/dark"
                    app:layout_constraintBottom_toBottomOf="@+id/registerMoreRestaurantActivitySpinnerCite"
                    app:layout_constraintStart_toStartOf="@+id/registerMoreRestaurantActivitySpinnerCite"
                    app:layout_constraintTop_toTopOf="@+id/registerMoreRestaurantActivitySpinnerCite" />


            </android.support.constraint.ConstraintLayout>

        </LinearLayout>
    </ScrollView>


</android.support.constraint.ConstraintLayout>