<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ContentDescription"
    android:layout_gravity="center"
    android:orientation="horizontal"
    >

    <RelativeLayout
        android:id="@+id/setAdapterItemsLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/dim_l6"
        android:layout_marginTop="@dimen/dim_l6"
        android:layout_marginEnd="@dimen/dim_24">


        <android.support.v7.widget.CardView
            android:id="@+id/setAdapterItemsCardView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dim_104"
            android:layout_marginStart="@dimen/dim_64"
            app:cardBackgroundColor="@color/gray77"
            app:cardCornerRadius="@dimen/dim_28"
            app:cardElevation="0dp"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">


                <LinearLayout
                    android:id="@+id/linearLayout5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dim_48"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/linearLayout3"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/setAdapterShowRestaurantClientTVNameRestaurant"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="TextView" />

                    <RatingBar
                        android:id="@+id/setAdapterShowRestaurantClientRatingBarRestaurant"
                        style="@style/Widget.AppCompat.RatingBar.Small"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:numStars="4"
                        android:rating="3"
                        android:scaleX=".6"
                        android:scaleY=".6"
                        android:transformPivotX="0dp"
                        android:transformPivotY="0dp" />

                    <TextView
                        android:id="@+id/setAdapterShowRestaurantClientTvMinimumRequest"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/items"
                        android:textColor="@color/dark"
                        android:textSize="@dimen/txt_size_12" />

                    <TextView
                        android:id="@+id/setAdapterShowRestaurantClientTvDeliveryCost"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:textSize="@dimen/txt_size_8" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_marginEnd="@dimen/dim_8">

                    <TextView
                        android:id="@+id/setAdapterShowRestaurantClientTvOnline"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:textSize="@dimen/txt_size_8" />

                    <ImageView
                        android:id="@+id/setAdapterShowRestaurantClientImgPhotoOnline"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:srcCompat="@android:drawable/ic_menu_mylocation" />

                </LinearLayout>


            </LinearLayout>
        </android.support.v7.widget.CardView>
        <LinearLayout

            android:layout_width="@dimen/dim_88"
            android:layout_height="@dimen/dim_88"
            android:layout_alignTop="@+id/setAdapterItemsCardView"
            android:layout_alignBottom="@+id/setAdapterItemsCardView"
            android:layout_alignParentStart="true"
            app:layout_constraintBottom_toBottomOf="@+id/setAdapterItemsCardView"
            app:layout_constraintEnd_toStartOf="@+id/setAdapterItemsCardView"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/setAdapterItemsCardView"
            app:layout_constraintVertical_bias="0.3"
            >

            <LinearLayout
                android:id="@+id/linearLayout6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/setAdapterShowRestaurantClientImgPhotoRestaurant"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textSize="@dimen/txt_size_20"
                    android:textStyle="bold" />

            </LinearLayout>
        </LinearLayout>

    </RelativeLayout>
</LinearLayout>