<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
android:layout_height="wrap_content"
app:cardBackgroundColor="@color/not_color"
app:cardElevation="0dp"


    >
    <android.support.v7.widget.CardView
        android:layout_width="@dimen/dim_250"
        android:layout_height="@dimen/dim_162"
        android:layout_marginTop="@dimen/dim_l6"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="@dimen/dim_28"
        app:cardElevation="0dp"
        android:layout_margin="@dimen/dim_8"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dim_28"
                android:text="@string/are_you_sure_about_going_out"
                android:textColor="@color/dark"
                android:gravity="center"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dim_28"
                >



                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/dim_28"
                    app:cardElevation="0dp"
                    android:layout_weight="1"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal"
                        >


                        <ImageView
                            android:id="@+id/closeDialogRestaurantClientBtnNo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/dim_l6"
                            android:src="@drawable/icon_multiply"
                            android:textColor="@color/dark"
                            android:textSize="@dimen/txt_size_20" />

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="@dimen/dim_28"
                    app:cardElevation="0dp"
                    android:layout_weight="1"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal"
                        >

                        <ImageView
                            android:id="@+id/closeDialogRestaurantClientBtnYes"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/dim_l6"
                            android:textColor="@color/dark"
                            android:src="@drawable/icon_mark"
                            />
                    </LinearLayout>
                </android.support.v7.widget.CardView>


            </LinearLayout>

        </LinearLayout>

    </android.support.v7.widget.CardView>
</android.support.v7.widget.CardView>


